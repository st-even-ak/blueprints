{namespace com.itg.plugin.confluence}
/**
 *  Job Request Properties
*/
{template .jobRequestProperties}
	<form action="#" id="jobRequestProperties" method="post" class="aui">
      	{call .labelInputPair}
      		{param strName}varProject{/param}
      		{param strId}varProject{/param}
      		{param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varProject.label')}{/param}
      		{param strRequiredText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varProject.required')}{/param}
      	{/call}
      	{call .labelInputPair}
      		{param strName}varTerritory{/param}
      		{param strId}varTerritory{/param}
      		{param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varTerritory.label')}{/param}
      		{param strRequiredText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varTerritory.required')}{/param}
      	{/call}
      	{call .labelInputPair}
      		{param strName}varTechLead{/param}
      		{param strId}varTechLead{/param}
      		{param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varTechLead.label')}{/param}
      		{param strRequiredText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varTechLead.required')}{/param}
      	{/call}
	</form>
{/template}
/**
*	Job Request Properties2
*/
