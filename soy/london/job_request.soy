/**
*	Job Steps
**/
{template .jobRequestJobSteps}
    {call .labelInputPair}
        {param strName}varJobName{/param}
        {param strId}varJobName{/param}
        {param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varJobName.label')}{/param}
        {param strRequiredText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varJobName.required')}{/param}
      	{param strHelpText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varJobName.helptext')}{/param}
    {/call}
    <p>Add the step details for this job below</p>
    <div>
        <table id="job-steps" name="job-steps" class="aui"></table>
    </div>

{/template}
