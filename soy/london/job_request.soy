            {param type}date{/param}
      		{param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varRemovalDate.label')}{/param}
      		{param strHelpText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varRemovalDate.helptext')}{/param}
      	{/call}
      	{call .labelInputPair}
      		{param strName}varServerDetails{/param}
      		{param strId}varServerDetails{/param}
      		{param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varServerDetails.label')}{/param}
      		{param strRequiredText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varServerDetails.required')}{/param}
      	{/call}
      	{call .labelInputPair}
      		{param strName}varNotificationEmail{/param}
      		{param strId}varNotificationEmail{/param}
      		{param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varNotificationEmail.label')}{/param}
      		{param strHelpText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varNotificationEmail.helptext')}{/param}
      	{/call}
      	{call .labelInputPair}
      		{param strName}varSupportDocuments{/param}
      		{param strId}varSupportDocuments{/param}
      		{param strLabel}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varSupportDocuments.label')}{/param}
      		{param strRequiredText}{getText('com.itg.plugin.confluence.jobRequestProperties.form.varSupportDocuments.required')}{/param}
      	{/call}
	</form>
{/template}
